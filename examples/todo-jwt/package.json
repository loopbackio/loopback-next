{
  "name": "@loopback/example-todo-jwt",
  "description": "A modified Todo tutorial how to build an application with JWT authentication and LoopBack 4",
  "version": "7.0.8",
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial",
    "CRUD",
    "models",
    "todo",
    "jwt",
    "authentication"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/todo-jwt"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-todo-jwt*.tgz dist *.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "verify": "npm pack && tar xf loopback-todo*.tgz && tree package && npm run clean",
    "premigrate": "npm run build ",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/authentication": "^12.0.8",
    "@loopback/authentication-jwt": "^0.16.8",
    "@loopback/boot": "^8.0.7",
    "@loopback/core": "^7.0.6",
    "@loopback/repository": "^8.0.6",
    "@loopback/rest": "^15.0.7",
    "@loopback/rest-explorer": "^8.0.7",
    "@loopback/security": "^0.12.7",
    "@loopback/service-proxy": "^8.0.6",
    "bcryptjs": "^3.0.3",
    "loopback-connector-rest": "^6.0.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.6",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/http-caching-proxy": "^7.0.6",
    "@loopback/testlab": "^8.0.6",
    "@types/lodash": "^4.17.20",
    "@types/node": "^16.18.126",
    "eslint": "^8.57.1",
    "lodash": "^4.17.21",
    "typescript": "~5.2.2"
  }
}
