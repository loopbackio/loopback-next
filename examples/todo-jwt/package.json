{
  "name": "@loopback/example-todo-jwt",
  "description": "A modified Todo tutorial how to build an application with JWT authentication and LoopBack 4",
  "version": "3.1.0",
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial",
    "CRUD",
    "models",
    "todo",
    "jwt",
    "authentication"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/todo-jwt"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-todo-jwt*.tgz dist *.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "verify": "npm pack && tar xf loopback-todo*.tgz && tree package && npm run clean",
    "premigrate": "npm run build ",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/authentication": "^8.1.0",
    "@loopback/authentication-jwt": "^0.11.0",
    "@loopback/boot": "^4.1.0",
    "@loopback/core": "^3.1.0",
    "@loopback/repository": "^4.1.0",
    "@loopback/rest": "^11.1.0",
    "@loopback/rest-explorer": "^4.1.0",
    "@loopback/security": "^0.7.0",
    "@loopback/service-proxy": "^4.1.0",
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "loopback-connector-rest": "^4.0.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.0",
    "@loopback/eslint-config": "^12.0.2",
    "@loopback/http-caching-proxy": "^3.1.0",
    "@loopback/testlab": "^4.1.0",
    "@types/lodash": "^4.14.179",
    "@types/node": "^12.20.46",
    "eslint": "^8.10.0",
    "lodash": "^4.17.21",
    "typescript": "~4.5.5"
  }
}
