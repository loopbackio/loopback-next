{
  "restApiRoot": "/",
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "handleUnknownPaths": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  }
}
