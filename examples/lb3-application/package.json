{
  "name": "@loopback/example-lb3-application",
  "version": "1.1.1",
  "description": "Tutorial example on how to add existing an LB3 application to a LB4 project",
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "author": "IBM Corp.",
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc es2017 --outDir dist --watch",
    "clean": "lb-clean *example-lb3-application*.tgz dist package api-docs",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "verify": "npm pack && tar xf loopback-lb3-application*.tgz && tree package && npm run clean",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@loopback/boot": "^1.4.0",
    "@loopback/booter-lb3app": "^1.2.1",
    "@loopback/context": "^1.19.0",
    "@loopback/core": "^1.8.1",
    "@loopback/repository": "^1.6.1",
    "@loopback/rest": "^1.15.0",
    "@loopback/rest-explorer": "^1.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "helmet": "^3.18.0",
    "loopback": "^3.25.1",
    "loopback-boot": "^3.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.7.1",
    "@loopback/eslint-config": "^1.1.1",
    "@loopback/rest": "^1.11.1",
    "@loopback/testlab": "^1.5.0",
    "@types/lodash": "^4.14.130",
    "@types/node": "^10.11.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^5.3.0",
    "lodash": "^4.17.11",
    "typescript": "~3.5.1"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example"
  ]
}
