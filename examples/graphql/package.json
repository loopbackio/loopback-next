{
  "name": "@loopback/example-graphql",
  "description": "GraphQL Example Application",
  "version": "0.12.2",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/graphql"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-graphql*.tgz dist *.tsbuildinfo package",
    "verify": "npm pack && tar xf *example-graphql*.tgz && tree package && npm run clean",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha dist/__tests__/**/*.js && npm run posttest",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node ."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^8.0.2",
    "@loopback/core": "^7.0.1",
    "@loopback/graphql": "^0.12.2",
    "@loopback/repository": "^8.0.1",
    "@loopback/rest": "^15.0.2",
    "class-transformer": "^0.5.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.1",
    "@loopback/eslint-config": "^16.0.0",
    "@loopback/testlab": "^8.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^16.18.126",
    "eslint": "^8.57.1",
    "graphql-ws": "^5.16.2",
    "rimraf": "^5.0.10",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
