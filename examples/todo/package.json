{
  "name": "@loopback/example-todo",
  "version": "0.9.3",
  "description": "Tutorial example on how to build an application with LoopBack 4.",
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "acceptance": "lb-mocha \"DIST/test/acceptance/**/*.js\"",
    "build": "npm run build:dist8 && npm run build:dist10",
    "build:apidocs": "lb-apidocs",
    "build:current": "lb-tsc",
    "build:dist8": "lb-tsc es2017",
    "build:dist10": "lb-tsc es2018",
    "clean": "lb-clean *example-todo*.tgz dist* package api-docs",
    "pretest": "npm run build:current",
    "test": "lb-mocha \"DIST/test/unit/**/*.js\" \"DIST/test/acceptance/**/*.js\"",
    "unit": "lb-mocha \"DIST/test/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-todo*.tgz && tree package && npm run clean",
    "prestart": "npm run build:current",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist*/src",
    "dist*/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^0.10.3",
    "@loopback/context": "^0.11.1",
    "@loopback/core": "^0.8.3",
    "@loopback/dist-util": "^0.3.1",
    "@loopback/openapi-v3": "^0.10.3",
    "@loopback/openapi-v3-types": "^0.7.3",
    "@loopback/repository": "^0.10.3",
    "@loopback/rest": "^0.10.3"
  },
  "devDependencies": {
    "@loopback/build": "^0.6.4",
    "@loopback/testlab": "^0.10.3",
    "@types/node": "^10.1.1",
    "source-map-support": "^0.5.4",
    "typescript": "^2.6.2"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial"
  ]
}
