{
  "name": "@loopback/example-metrics-prometheus",
  "version": "0.2.8",
  "description": "Standalone examples for @loopback/metrics",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "author": "IBM Corp.",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-metrics-prometheus*.tgz dist *.tsbuildinfo package",
    "verify": "npm pack && tar xf *example-metrics-prometheus*.tgz && tree package && npm run clean",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha dist/__tests__/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node .",
    "demo": "./bin/demo.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "examples/metrics-prometheus"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@loopback/boot": "^2.2.0",
    "@loopback/core": "^2.5.0",
    "@loopback/extension-metrics": "^0.3.3",
    "@loopback/rest": "^4.0.0",
    "tslib": "^1.12.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.3.1",
    "@loopback/eslint-config": "^6.0.6",
    "@loopback/testlab": "^3.1.3",
    "@types/node": "^10.17.21",
    "eslint": "^7.0.0",
    "typescript": "~3.8.3"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial"
  ]
}
