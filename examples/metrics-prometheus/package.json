{
  "name": "@loopback/example-metrics-prometheus",
  "version": "0.1.0",
  "description": "Standalone examples for @loopback/metrics",
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "author": "IBM Corp.",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-metrics-prometheus*.tgz dist tsconfig.build.tsbuildinfo package",
    "verify": "npm pack && tar xf *example-metrics-prometheus*.tgz && tree package && npm run clean",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha dist/__tests__/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node .",
    "demo": "./bin/demo.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "examples/metrics-prometheus"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@loopback/boot": "^1.5.11",
    "@loopback/core": "^1.10.7",
    "@loopback/extension-metrics": "^0.1.0",
    "@loopback/rest": "^1.23.0"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.16",
    "@loopback/eslint-config": "^4.1.4",
    "@loopback/testlab": "^1.9.4",
    "@types/node": "^10.17.5",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.2.2",
    "typescript": "~3.7.2"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial"
  ]
}
