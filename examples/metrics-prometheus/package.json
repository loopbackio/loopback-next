{
  "name": "@loopback/example-metrics-prometheus",
  "description": "An example to illustrate metrics provided by @loopback/metrics and Prometheus",
  "version": "0.10.0",
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/metrics-prometheus"
  },
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-metrics-prometheus*.tgz dist *.tsbuildinfo package",
    "verify": "npm pack && tar xf *example-metrics-prometheus*.tgz && tree package && npm run clean",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha dist/__tests__/**/*.js && npm run posttest",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node .",
    "demo": "./bin/demo.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/boot": "^5.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/metrics": "^0.11.0",
    "@loopback/rest": "^12.0.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.0",
    "@loopback/eslint-config": "^13.0.0",
    "@loopback/testlab": "^5.0.0",
    "@types/node": "^14.18.20",
    "eslint": "^8.16.0",
    "typescript": "~4.7.2"
  }
}
