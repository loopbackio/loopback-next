{
  "name": "@loopback/example-webpack",
  "description": "An example to bundle @loopback/core using webpack",
  "version": "0.5.2",
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "unpkg": "dist/bundle-web.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/webpack"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:webpack-node": "webpack --config-name node",
    "build:webpack-web": "webpack --config-name web",
    "prepack": "npm run build:webpack-web",
    "clean": "lb-clean loopback-example-webpack*.tgz dist *.tsbuildinfo package",
    "verify": "npm pack && tar xf loopback-example-webpack*.tgz && tree package && npm run clean",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "rebuild": "npm run clean && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/core": "^2.18.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.2",
    "@loopback/eslint-config": "^11.0.2",
    "@loopback/testlab": "^3.4.4",
    "@types/node": "^10.17.60",
    "@types/puppeteer": "^5.4.4",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "eslint": "^7.32.0",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "puppeteer": "^10.4.0",
    "typescript": "~4.4.4",
    "util": "^0.12.4",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  }
}
