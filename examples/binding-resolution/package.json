{
  "name": "@loopback/example-binding-resolution",
  "description": "An example application to demonstrate binding resolution and dependency injection within a context hierarchy",
  "version": "0.6.1",
  "keywords": [
    "loopback-application",
    "loopback",
    "IoC",
    "dependency injection",
    "context",
    "binding"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/binding-resolution"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^4.1.1",
    "@loopback/core": "^3.1.1",
    "@loopback/rest": "^11.1.1",
    "@loopback/rest-explorer": "^4.1.1",
    "debug": "^4.3.3",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.1",
    "@loopback/testlab": "^4.1.1",
    "@types/debug": "^4.1.7",
    "@types/node": "^12.20.46",
    "eslint": "^8.10.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.5.5"
  }
}
