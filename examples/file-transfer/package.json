{
  "name": "@loopback/example-file-transfer",
  "version": "1.2.3",
  "description": "Example application for file upload/download with LoopBack 4",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "author": "IBM Corp.",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-file-transfer*.tgz dist *.tsbuildinfo package",
    "verify": "npm pack && tar xf *example-file-transfer*.tgz && tree package && npm run clean",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha dist/__tests__/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "examples/file-transfer"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@loopback/boot": "^2.1.1",
    "@loopback/context": "^3.5.1",
    "@loopback/core": "^2.4.1",
    "@loopback/openapi-v3": "^3.2.1",
    "@loopback/rest": "^3.3.1",
    "@loopback/rest-explorer": "^2.1.1",
    "multer": "^1.4.2",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@loopback/build": "^5.2.0",
    "@loopback/eslint-config": "^6.0.4",
    "@loopback/testlab": "^3.1.1",
    "@types/express-serve-static-core": "^4.17.5",
    "@types/multer": "^1.4.3",
    "@types/node": "^10.17.21",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "typescript": "~3.8.3"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "file",
    "upload"
  ]
}
