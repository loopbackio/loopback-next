{
  "name": "@loopback/example-soap-calculator",
  "version": "0.5.0",
  "description": "Integrate a SOAP webservice with LoopBack 4",
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial",
    "soap"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc es2017 --outDir dist --watch",
    "clean": "lb-clean *example-soap*.tgz dist package api-docs dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node ."
  },
  "repository": {
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@loopback/boot": "^0.14.7",
    "@loopback/context": "^0.13.2",
    "@loopback/core": "^0.11.17",
    "@loopback/openapi-v3": "^0.15.6",
    "@loopback/repository": "^0.21.2",
    "@loopback/rest": "^0.26.0",
    "@loopback/service-proxy": "^0.8.11",
    "loopback-connector-soap": "^5.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^0.8.0",
    "@loopback/testlab": "^0.15.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.11.2",
    "mocha": "^5.1.1",
    "source-map-support": "^0.5.5"
  }
}
