{
  "name": "@loopback/example-socketio",
  "description": "This is an example for spinning up socket server.",
  "version": "0.11.8",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/socketio"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t socketio .",
    "docker:run": "docker run -p 3000:3000 -d socketio",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^8.0.8",
    "@loopback/core": "^7.0.7",
    "@loopback/rest": "^15.0.8",
    "@loopback/rest-explorer": "^8.0.8",
    "@loopback/socketio": "^0.9.8",
    "debug": "^4.4.3",
    "p-event": "^4.2.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.7",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/testlab": "^8.0.7",
    "@types/debug": "^4.1.12",
    "@types/node": "^16.18.126",
    "eslint": "^8.57.1",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
