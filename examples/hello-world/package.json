{
  "name": "@loopback/example-hello-world",
  "description": "A simple hello-world Application using LoopBack 4",
  "version": "8.0.7",
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "examples/hello-world"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-hello-world*.tgz dist *.tsbuildinfo package",
    "verify": "npm pack && tar xf *example-hello-world*.tgz && tree package && npm run clean",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__/**/*.js\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/core": "^7.0.6",
    "@loopback/rest": "^15.0.7",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.6",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/testlab": "^8.0.6",
    "@types/node": "^16.18.126",
    "eslint": "^8.57.1",
    "typescript": "~5.2.2"
  }
}
