{
  "name": "@loopback/example-hello-world",
  "version": "2.0.8",
  "description": "A simple hello-world Application using LoopBack 4",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "author": "IBM Corp.",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-hello-world*.tgz dist *.tsbuildinfo package",
    "verify": "npm pack && tar xf *example-hello-world*.tgz && tree package && npm run clean",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__/**/*.js\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "examples/hello-world"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@loopback/core": "^2.5.0",
    "@loopback/rest": "^4.0.0",
    "tslib": "^1.11.2"
  },
  "devDependencies": {
    "@loopback/build": "^5.3.1",
    "@loopback/eslint-config": "^6.0.6",
    "@loopback/testlab": "^3.1.3",
    "@types/node": "^10.17.21",
    "eslint": "^7.0.0",
    "typescript": "~3.8.3"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial"
  ]
}
