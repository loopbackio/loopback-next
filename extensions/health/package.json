{
  "name": "@loopback/health",
  "description": "An extension exposes health check related endpoints with LoopBack 4",
  "version": "0.11.0",
  "keywords": [
    "LoopBack",
    "Cloud Native",
    "Health"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/health"
  },
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-extension-health*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-extension-health*.tgz && tree package && npm run clean",
    "demo": "./src/__examples__/demo.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^4.0.0",
    "@loopback/rest": "^12.0.0"
  },
  "dependencies": {
    "@cloudnative/health": "^2.1.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/eslint-config": "^13.0.0",
    "@loopback/rest": "^12.0.0",
    "@loopback/testlab": "^5.0.0",
    "@types/node": "^14.18.17"
  }
}
