{
  "name": "@loopback/health",
  "description": "An extension exposes health check related endpoints with LoopBack 4",
  "version": "0.15.1",
  "keywords": [
    "LoopBack",
    "Cloud Native",
    "Health"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/health"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-extension-health*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-extension-health*.tgz && tree package && npm run clean",
    "demo": "./src/__examples__/demo.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^7.0.0",
    "@loopback/rest": "^15.0.1"
  },
  "dependencies": {
    "@cloudnative/health": "^2.1.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.1",
    "@loopback/core": "^7.0.1",
    "@loopback/eslint-config": "^16.0.0",
    "@loopback/rest": "^15.0.1",
    "@loopback/testlab": "^8.0.1",
    "@types/node": "^16.18.126"
  }
}
