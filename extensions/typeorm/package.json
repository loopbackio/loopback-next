{
  "name": "@loopback/typeorm",
  "description": "Adds support for TypeORM in LoopBack",
  "version": "0.8.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/typeorm"
  },
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-typeorm*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-typeorm*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/boot": "^6.0.0",
    "@loopback/core": "^5.0.0",
    "@loopback/rest": "^13.0.0"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "tslib": "^2.5.2",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@loopback/boot": "^6.0.0",
    "@loopback/build": "^10.0.0",
    "@loopback/core": "^5.0.0",
    "@loopback/eslint-config": "^14.0.0",
    "@loopback/repository": "^6.0.0",
    "@loopback/rest": "^13.0.0",
    "@loopback/testlab": "^6.0.0",
    "@types/debug": "^4.1.8",
    "@types/json-schema": "^7.0.12",
    "@types/node": "^16.18.34",
    "sqlite3": "^5.1.6"
  }
}
