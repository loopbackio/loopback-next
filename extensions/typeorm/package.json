{
  "name": "@loopback/typeorm",
  "description": "Adds support for TypeORM in LoopBack",
  "version": "0.7.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/typeorm"
  },
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-typeorm*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-typeorm*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/boot": "^5.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/rest": "^12.0.0"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "tslib": "^2.4.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@loopback/boot": "^5.0.0",
    "@loopback/build": "^9.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/eslint-config": "^13.0.0",
    "@loopback/repository": "^5.0.0",
    "@loopback/rest": "^12.0.0",
    "@loopback/testlab": "^5.0.0",
    "@types/debug": "^4.1.7",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^14.18.20",
    "sqlite3": "^5.0.8"
  }
}
