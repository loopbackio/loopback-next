{
  "name": "@loopback/graphql",
  "description": "LoopBack's graphql integration",
  "version": "0.11.1",
  "keywords": [
    "LoopBack",
    "GraphQL"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/graphql"
  },
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-graphql*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-graphql*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/boot": "^7.0.0",
    "@loopback/core": "^6.0.0"
  },
  "dependencies": {
    "@graphql-tools/utils": "^10.2.0",
    "@loopback/http-server": "^6.0.1",
    "apollo-server-express": "^3.12.1",
    "debug": "^4.3.4",
    "express": "^4.19.2",
    "graphql": "^15.8.0",
    "graphql-subscriptions": "^2.0.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@loopback/boot": "^7.0.1",
    "@loopback/build": "^11.0.1",
    "@loopback/core": "^6.0.1",
    "@loopback/eslint-config": "^15.0.1",
    "@loopback/repository": "^7.0.1",
    "@loopback/rest": "^14.0.1",
    "@loopback/testlab": "^7.0.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^16.18.96",
    "class-transformer": "^0.5.1"
  }
}
