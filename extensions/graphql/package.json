{
  "name": "@loopback/graphql",
  "description": "LoopBack's graphql integration",
  "version": "0.12.3",
  "keywords": [
    "LoopBack",
    "GraphQL"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/graphql"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-graphql*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-graphql*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/boot": "^8.0.1",
    "@loopback/core": "^7.0.0",
    "@loopback/rest": "^15.0.1"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@graphql-tools/utils": "^10.9.0",
    "@loopback/http-server": "^7.0.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.2",
    "type-graphql": "^2.0.0-beta.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@loopback/boot": "^8.0.3",
    "@loopback/build": "^12.0.2",
    "@loopback/core": "^7.0.2",
    "@loopback/eslint-config": "^16.0.0",
    "@loopback/repository": "^8.0.2",
    "@loopback/rest": "^15.0.3",
    "@loopback/testlab": "^8.0.2",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.12",
    "@types/node": "^16.18.126",
    "@types/ws": "^8.18.1",
    "class-transformer": "^0.5.1"
  }
}
