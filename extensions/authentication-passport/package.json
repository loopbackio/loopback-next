{
  "name": "@loopback/authentication-passport",
  "version": "2.0.0",
  "description": "A package creating adapters between the passport module and @loopback/authentication",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-authentication-passport*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-authentication-passport*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "keywords": [
    "Passport",
    "Authentication",
    "TypeScript"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/authentication-passport"
  },
  "dependencies": {
    "@loopback/authentication": "^4.0.0",
    "@loopback/context": "^3.0.0",
    "@loopback/core": "^2.0.0",
    "@loopback/metadata": "^2.0.0",
    "@loopback/openapi-v3": "^3.0.0",
    "@loopback/rest": "^3.0.0",
    "@loopback/security": "^0.2.0",
    "passport": "^0.4.1",
    "tslib": "^1.11.1",
    "util-promisifyall": "^1.0.6"
  },
  "devDependencies": {
    "@loopback/build": "^4.0.0",
    "@loopback/eslint-config": "^6.0.0",
    "@loopback/openapi-spec-builder": "^2.0.0",
    "@loopback/testlab": "^2.0.0",
    "@types/node": "^10.17.17",
    "@types/passport": "^1.0.3",
    "@types/passport-http": "^0.3.8",
    "passport-http": "^0.3.0"
  }
}
