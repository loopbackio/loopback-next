{
  "name": "@loopback/authentication-passport",
  "description": "A package creating adapters between the passport module and @loopback/authentication",
  "version": "8.0.8",
  "keywords": [
    "Passport",
    "Authentication",
    "TypeScript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/authentication-passport"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-authentication-passport*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-authentication-passport*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/authentication": "^12.0.1",
    "@loopback/core": "^7.0.0",
    "@loopback/rest": "^15.0.1"
  },
  "dependencies": {
    "@loopback/security": "^0.12.7",
    "passport": "^0.7.0",
    "tslib": "^2.8.1",
    "util-promisifyall": "^1.0.6"
  },
  "devDependencies": {
    "@loopback/authentication": "^12.0.8",
    "@loopback/build": "^12.0.6",
    "@loopback/core": "^7.0.6",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/mock-oauth2-provider": "^0.10.6",
    "@loopback/openapi-spec-builder": "^8.0.6",
    "@loopback/rest": "^15.0.7",
    "@loopback/testlab": "^8.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/node": "^16.18.126",
    "@types/passport": "^1.0.17",
    "@types/passport-http": "^0.3.11",
    "@types/passport-oauth2": "^1.8.0",
    "@types/qs": "^6.14.0",
    "axios": "^1.13.2",
    "body-parser": "^2.2.1",
    "express": "^4.21.2",
    "form-data": "^4.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "passport-http": "^0.3.0",
    "passport-oauth2": "^1.8.0",
    "qs": "^6.14.0",
    "supertest": "^7.1.4"
  }
}
