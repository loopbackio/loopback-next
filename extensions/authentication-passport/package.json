{
  "name": "@loopback/authentication-passport",
  "description": "A package creating adapters between the passport module and @loopback/authentication",
  "version": "4.1.2",
  "keywords": [
    "Passport",
    "Authentication",
    "TypeScript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/authentication-passport"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17 || 18"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-authentication-passport*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-authentication-passport*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/authentication": "^8.1.2",
    "@loopback/core": "^3.1.2",
    "@loopback/rest": "^11.1.2"
  },
  "dependencies": {
    "@loopback/security": "^0.7.2",
    "passport": "^0.5.2",
    "tslib": "^2.4.0",
    "util-promisifyall": "^1.0.6"
  },
  "devDependencies": {
    "@loopback/authentication": "^8.1.2",
    "@loopback/build": "^8.1.2",
    "@loopback/core": "^3.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@loopback/mock-oauth2-provider": "^0.5.2",
    "@loopback/openapi-spec-builder": "^4.1.2",
    "@loopback/rest": "^11.1.2",
    "@loopback/testlab": "^4.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.20.50",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "@types/passport-oauth2": "^1.4.11",
    "@types/qs": "^6.9.7",
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "passport-http": "^0.3.0",
    "passport-oauth2": "^1.6.1",
    "qs": "^6.10.3",
    "supertest": "^6.2.3"
  }
