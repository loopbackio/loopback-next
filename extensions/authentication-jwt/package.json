    "@loopback/core": "^2.10.1",
    "@loopback/rest": "^7.0.1"
  },
  "dependencies": {
    "@loopback/security": "^0.3.1",
    "@types/bcryptjs": "2.4.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@loopback/authentication": "^7.0.1",
    "@loopback/boot": "^3.0.1",
    "@loopback/build": "^6.2.4",
    "@loopback/core": "^2.10.1",
    "@loopback/eslint-config": "^10.0.0",
