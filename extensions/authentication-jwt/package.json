{
  "name": "@loopback/authentication-jwt",
  "description": "Extension for the prototype of JWT authentication",
  "version": "0.14.1",
  "keywords": [
    "LoopBack",
    "Authentication",
    "jsonwebtoken"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/authentication-jwt"
  },
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-authentication-jwt*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/unit/*.js\" \"dist/__tests__/acceptance/*.js\"",
    "verify": "npm pack && tar xf loopback-authentication-jwt*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/authentication": "^10.0.0",
    "@loopback/core": "^5.0.0",
    "@loopback/rest": "^13.0.0"
  },
  "dependencies": {
    "@loopback/security": "^0.10.1",
    "@types/bcryptjs": "2.4.2",
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.4",
    "jsonwebtoken": "^9.0.1"
  },
  "devDependencies": {
    "@loopback/authentication": "^10.1.1",
    "@loopback/boot": "^6.1.1",
    "@loopback/build": "^10.1.1",
    "@loopback/core": "^5.1.1",
    "@loopback/eslint-config": "^14.0.2",
    "@loopback/repository": "^6.1.1",
    "@loopback/rest": "^13.1.1",
    "@loopback/rest-explorer": "^6.1.1",
    "@loopback/service-proxy": "^6.1.1",
    "@loopback/testlab": "^6.1.1",
    "@types/debug": "^4.1.8",
    "@types/lodash": "^4.14.196",
    "@types/node": "^16.18.40",
    "lodash": "^4.17.21",
    "typescript": "~5.1.6"
  }
}
