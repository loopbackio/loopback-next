{
  "name": "@loopback/authentication-jwt",
  "description": "Extension for the prototype of JWT authentication",
  "version": "0.15.3",
  "keywords": [
    "LoopBack",
    "Authentication",
    "jsonwebtoken"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/authentication-jwt"
  },
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-authentication-jwt*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/unit/*.js\" \"dist/__tests__/acceptance/*.js\"",
    "verify": "npm pack && tar xf loopback-authentication-jwt*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/authentication": "^11.0.0",
    "@loopback/core": "^6.0.0",
    "@loopback/rest": "^14.0.0"
  },
  "dependencies": {
    "@loopback/security": "^0.11.3",
    "@types/bcryptjs": "2.4.6",
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.5",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@loopback/authentication": "^11.0.3",
    "@loopback/boot": "^7.0.3",
    "@loopback/build": "^11.0.3",
    "@loopback/core": "^6.1.0",
    "@loopback/eslint-config": "^15.0.3",
    "@loopback/repository": "^7.0.3",
    "@loopback/rest": "^14.0.3",
    "@loopback/rest-explorer": "^7.0.3",
    "@loopback/service-proxy": "^7.0.3",
