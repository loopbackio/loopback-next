{
  "name": "@loopback/prisma",
  "version": "0.1.0",
  "description": "@loopback/prisma",
  "keywords": [
    "loopback-extension",
    "loopback",
    "prisma",
    "orm",
    "odm"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "npm run build:prisma && lb-tsc",
    "build:prisma": "prisma generate --schema ./src/__tests__/fixtures/schema.prisma",
    "build:watch": "lb-tsc --watch",
    "pretest": "npm run rebuild",
    "test": "lb-mocha dist/__tests__/**/*.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/prisma"
  },
  "author": "LoopBack Contributors",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^2.15.1",
    "@loopback/logging": "^0.6.0",
    "@loopback/testlab": "^3.3.1",
    "@prisma/client": "^3.11.1"
  },
  "peerDependenciesMeta": {
    "@loopback/logging": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.1",
    "@loopback/core": "^2.15.1",
    "@loopback/logging": "^0.6.0",
    "@loopback/repository": "^3.5.1",
    "@loopback/testlab": "^3.3.1",
    "@prisma/client": "3.11.1",
    "@types/node": "^10.17.56",
    "mocha-steps": "^1.3.0",
    "prisma": "^3.11.1"
  },
  "copyright.owner": "The LoopBack Authors",
  "publishConfig": {
    "access": "public"
  }
}
