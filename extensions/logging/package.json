{
  "name": "@loopback/extension-logging",
  "version": "0.3.1",
  "description": "LoopBack Logging for Winston and Fluentd",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-extension-logging*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-extension-logging*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^3.5.1",
    "@loopback/core": "^2.4.1",
    "@loopback/rest": "^3.3.1",
    "fluent-logger": "^3.4.1",
    "morgan": "^1.10.0",
    "tslib": "^1.11.1",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.2.0",
    "@loopback/eslint-config": "^6.0.4",
    "@loopback/testlab": "^3.1.1",
    "@types/morgan": "^1.9.0",
    "@types/node": "^10.17.21",
    "p-event": "^4.1.0"
  },
  "keywords": [
    "LoopBack",
    "Cloud Native",
    "Fluentd",
    "Winston",
    "Logging"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__",
    "templates"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/logging"
  }
}
