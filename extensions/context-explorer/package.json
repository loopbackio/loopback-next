{
  "name": "@loopback/context-explorer",
  "description": "Visualize context hierarchy, bindings, configurations, and dependencies",
  "version": "0.12.5",
  "keywords": [
    "LoopBack",
    "Explorer",
    "Context",
    "Binding"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/context-explorer"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-explorer*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-context-explorer*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "public",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^7.0.0",
    "@loopback/rest": "^15.0.1"
  },
  "dependencies": {
    "ts-graphviz": "^3.0.2",
    "viz.js": "^2.1.2"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.4",
    "@loopback/core": "^7.0.4",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/rest": "^15.0.5",
    "@loopback/testlab": "^8.0.4",
    "@types/node": "^16.18.126"
  }
}
