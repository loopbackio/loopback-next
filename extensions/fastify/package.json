{
  "name": "@loopback/fastify",
  "version": "0.0.1",
  "description": "A lightweight REST server powered by Fastify",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/fastify"
  },
  "author": "IBM Corp.",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/openapi-v3": "^3.3.1",
    "@openapi-contrib/openapi-schema-to-json-schema": "^3.0.0",
    "debug": "^4.1.1",
    "fastify": "^2.14.1",
    "http-errors": "^1.7.3",
    "tslib": "^1.10.0"
  },
  "peerDependencies": {
    "@loopback/core": "^2.5.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.3.1",
    "@loopback/core": "^2.5.0",
    "@loopback/eslint-config": "^6.0.6",
    "@loopback/openapi-spec-builder": "^2.1.3",
    "@loopback/testlab": "^3.1.3",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.21",
    "@types/http-errors": "^1.6.3"
  },
  "copyright.owner": "IBM Corp.",
  "publishConfig": {
    "access": "public"
  }
