{
  "name": "@loopback/apiconnect",
  "description": "An extension for IBM API Connect",
  "version": "0.14.8",
  "keywords": [
    "LoopBack",
    "IBM API Connect"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/apiconnect"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-apiconnect*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-apiconnect*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^7.0.0",
    "@loopback/rest": "^15.0.1"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.7",
    "@loopback/core": "^7.0.7",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/rest": "^15.0.8",
    "@loopback/testlab": "^8.0.7",
    "@types/node": "^16.18.126"
  }
}
