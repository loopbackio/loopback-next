{
  "name": "@loopback/mock-oauth2-provider",
  "description": "An internal application to mock the OAuth2 authorization flow login with a social app like facebook, google etc",
  "version": "0.5.2",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "fixtures/mock-oauth2-provider"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-mock-oauth2-provider*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "prestart": "npm run build",
    "start": "node .",
    "test": "npm run mocha",
    "mocha": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-mock-oauth2-provider*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.20.50",
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@loopback/testlab": "^4.1.2"
  }
}
