{
  "name": "@loopback/mock-oauth2-provider",
  "description": "An internal application to mock the OAuth2 authorization flow login with a social app like facebook, google etc",
  "version": "0.10.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "fixtures/mock-oauth2-provider"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-mock-oauth2-provider*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "prestart": "npm run build",
    "start": "node .",
    "test": "npm run mocha",
    "mocha": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-mock-oauth2-provider*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.18",
    "@types/node": "^16.18.126",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "express": "^4.21.2",
    "form-data": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.1",
    "@loopback/eslint-config": "^16.0.0",
    "@loopback/testlab": "^8.0.1"
  }
}
