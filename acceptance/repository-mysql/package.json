{
  "name": "@loopback/test-repository-mysql",
  "description": "Acceptance tests for `@loopback/repository` + `loopback-connector-mysql`",
  "version": "0.15.14",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "acceptance/repository-mysql"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-test-repository-mysql*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "npm run mocha",
    "mocha": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-test-repository-mysql*.tgz && tree package && npm run clean"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "devDependencies": {
    "@loopback/build": "^11.0.12",
    "@loopback/eslint-config": "^15.0.5",
    "@loopback/repository": "^7.0.14",
    "@loopback/repository-tests": "^0.24.14",
    "@loopback/testlab": "^7.0.13",
    "@types/node": "^22.15.30",
    "loopback-connector-mysql": "^7.1.3",
    "tslib": "^2.8.1"
  }
}
