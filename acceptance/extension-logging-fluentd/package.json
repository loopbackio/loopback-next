    "@loopback/extension-logging": "^0.0.1",
    "testcontainers": "^2.1.0",
    "@types/node": "^10.17.5"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
