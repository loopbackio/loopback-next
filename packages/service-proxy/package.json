{
  "name": "@loopback/service-proxy",
  "version": "1.3.14",
  "description": "Service integration for LoopBack 4",
  "engines": {
    "node": ">=8.9"
  },
  "main": "index",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-service-proxy*.tgz dist tsconfig.build.tsbuildinfo package",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-service-proxy*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "devDependencies": {
    "@loopback/build": "^3.0.1",
    "@loopback/eslint-config": "^5.0.1",
    "@loopback/testlab": "^1.10.1",
    "@types/node": "^10.17.13"
  },
  "dependencies": {
    "@loopback/context": "^1.25.1",
    "@loopback/core": "^1.12.1",
    "loopback-datasource-juggler": "^4.18.0"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/service-proxy"
  }
}
