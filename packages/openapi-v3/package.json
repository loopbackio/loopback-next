{
  "name": "@loopback/openapi-v3",
  "version": "1.9.1",
  "description": "Processes openapi v3 related metadata",
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@loopback/context": "^1.21.1",
    "@loopback/repository-json-schema": "^1.9.2",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "openapi3-ts": "^1.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.6",
    "@loopback/eslint-config": "^3.0.0",
    "@loopback/openapi-spec-builder": "^1.2.8",
    "@loopback/repository": "^1.10.1",
    "@loopback/testlab": "^1.7.2",
    "@types/debug": "^4.1.4",
    "@types/lodash": "^4.14.136",
    "@types/node": "^10.14.15"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-openapi-v3*.tgz dist tsconfig.build.tsbuildinfo package",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-openapi-v3*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "keywords": [
    "Swagger",
    "OpenAPI Spec",
    "TypeScript"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/openapi-v3"
  }
}
