{
  "name": "@loopback/openapi-v3",
  "description": "Decorators that annotate LoopBack artifacts with OpenAPI v3 metadata and utilities that transform LoopBack metadata to OpenAPI v3 specifications",
  "version": "11.0.8",
  "keywords": [
    "Swagger",
    "OpenAPI Spec",
    "TypeScript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/openapi-v3"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-openapi-v3*.tgz dist *.tsbuildinfo package",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-openapi-v3*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^7.0.0"
  },
  "dependencies": {
    "@loopback/repository-json-schema": "^9.0.8",
    "debug": "^4.4.3",
    "http-status": "^1.8.1",
    "json-merge-patch": "^1.0.2",
    "lodash": "^4.17.21",
    "openapi3-ts": "^2.0.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.7",
    "@loopback/core": "^7.0.7",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/openapi-spec-builder": "^8.0.7",
    "@loopback/repository": "^8.0.7",
    "@loopback/testlab": "^8.0.7",
    "@types/debug": "^4.1.12",
    "@types/json-merge-patch": "1.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^16.18.126"
  }
}
