{
  "name": "@loopback/repository",
  "version": "1.1.0",
  "description": "Repository based persistence for LoopBack 4",
  "engines": {
    "node": ">=8.9"
  },
  "main": "index",
  "scripts": {
    "acceptance": "lb-mocha \"dist/test/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "clean": "lb-clean loopback-repository*.tgz dist package api-docs",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/test/**/*.js\"",
    "verify": "npm pack && tar xf loopback-repository*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "devDependencies": {
    "@loopback/build": "^1.0.2",
    "@loopback/testlab": "^1.0.2",
    "@loopback/tslint-config": "^1.0.0-1",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.11.2"
  },
  "dependencies": {
    "@loopback/context": "^1.3.0",
    "@loopback/core": "^1.1.2",
    "@types/debug": "0.0.30",
    "debug": "^4.0.1",
    "lodash": "^4.17.10",
    "loopback-datasource-juggler": "^4.0.0"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
