  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@loopback/core": "^2.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.3",
    "@loopback/core": "^2.10.0",
    "@loopback/eslint-config": "^10.0.0",
    "@loopback/testlab": "^3.2.5",
    "@types/bson": "^4.0.2",
    "@types/json-schema": "^7.0.6",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.17.35",
    "bson": "4.1.0"
  },
  "dependencies": {
    "@loopback/filter": "^1.1.0",
    "@types/debug": "^4.1.5",
    "debug": "^4.1.1",
    "lodash": "^4.17.20",
    "loopback-datasource-juggler": "^4.24.0",
    "tslib": "^2.0.1"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/repository"
  }
