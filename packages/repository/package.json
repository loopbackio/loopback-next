{
  "name": "@loopback/repository",
  "description": "Define and implement a common set of interfaces for interacting with databases",
  "version": "6.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/repository"
  },
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-repository*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-repository*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^5.0.0"
  },
  "dependencies": {
    "@loopback/filter": "^4.1.1",
    "@types/debug": "^4.1.8",
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "loopback-datasource-juggler": "^4.28.9",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.1",
    "@loopback/core": "^5.1.1",
    "@loopback/eslint-config": "^14.0.2",
    "@loopback/testlab": "^6.1.1",
    "@types/json-schema": "^7.0.12",
    "@types/lodash": "^4.14.197",
    "@types/node": "^16.18.40",
    "bson": "5.4.0"
  }
}
