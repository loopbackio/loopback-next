{
  "name": "@loopback/repository-json-schema",
  "description": "Converts TS classes into JSON Schemas using TypeScript's reflection API",
  "version": "4.0.0",
  "keywords": [
    "LoopBack",
    "TypeScript",
    "JSON Schema"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/repository-json-schema"
  },
  "engines": {
    "node": "^10.16 || 12 || 14 || 16"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-json-schema*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-json-schema*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^2.16.2",
    "@loopback/repository": "^3.7.1"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.7",
    "debug": "^4.3.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/core": "^2.16.2",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/repository": "^3.7.1",
    "@loopback/testlab": "^3.4.2",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.60",
    "ajv-formats": "^2.1.0"
  }
}
