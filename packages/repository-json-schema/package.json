{
  "name": "@loopback/repository-json-schema",
  "version": "2.4.6",
  "description": "Converts TS classes into JSON Schemas using TypeScript's reflection API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-json-schema*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-json-schema*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "LoopBack",
    "TypeScript",
    "JSON Schema"
  ],
  "peerDependencies": {
    "@loopback/core": "^2.9.1",
    "@loopback/repository": "^2.9.0"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.5",
    "debug": "^4.1.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.1.0",
    "@loopback/core": "^2.9.1",
    "@loopback/eslint-config": "^8.0.3",
    "@loopback/repository": "^2.9.0",
    "@loopback/testlab": "^3.2.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.27",
    "ajv": "^6.12.3"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/repository-json-schema"
  }
}
