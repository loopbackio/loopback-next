{
  "name": "@loopback/repository-json-schema",
  "version": "1.11.0",
  "description": "Converts TS classes into JSON Schemas using TypeScript's reflection API",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-json-schema*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-json-schema*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "license": "MIT",
  "keywords": [
    "LoopBack",
    "TypeScript",
    "JSON Schema"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/context": "^1.23.4",
    "@loopback/metadata": "^1.3.6",
    "@loopback/repository": "^1.15.3",
    "@types/json-schema": "^7.0.3",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.15",
    "@loopback/eslint-config": "^4.1.3",
    "@loopback/testlab": "^1.9.3",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.3",
    "ajv": "^6.10.2"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/repository-json-schema"
  }
}
