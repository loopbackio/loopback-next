{
  "name": "@loopback/testlab",
  "version": "0.8.0",
  "description": "A collection of test utilities we use to write LoopBack tests.",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "npm run build:dist8 && npm run build:dist10",
    "build:current": "lb-tsc",
    "build:dist8": "lb-tsc es2017",
    "build:dist10": "lb-tsc es2018",
    "clean": "lb-clean loopback-testlab*.tgz dist* package api-docs",
    "pretest": "npm run build:current",
    "test": "lb-mocha \"DIST/test\"",
    "verify": "npm pack && tar xf loopback-testlab*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/dist-util": "^0.1.0",
    "@types/fs-extra": "^5.0.1",
    "@types/shot": "^3.4.0",
    "@types/sinon": "^4.3.0",
    "@types/supertest": "^2.0.4",
    "fs-extra": "^5.0.0",
    "shot": "^4.0.5",
    "should": "^13.2.1",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0",
    "swagger2openapi": "^2.11.16"
  },
  "devDependencies": {
    "@loopback/build": "^0.6.0",
    "@types/node": "^8.10.4"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist*/src",
    "dist*/index*",
    "should-as-function.d.ts",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
