{
  "name": "@loopback/testlab",
  "version": "1.7.4",
  "description": "A collection of test utilities we use to write LoopBack tests.",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-testlab*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-testlab*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@hapi/shot": "^4.1.1",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.0",
    "@types/shot": "^4.0.0",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.8",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "oas-validator": "^3.3.1",
    "should": "^13.2.3",
    "sinon": "^7.4.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.8",
    "@loopback/eslint-config": "^4.0.1",
    "@types/node": "^10.14.16"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "fixtures",
    "should-as-function.d.ts",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/testlab"
  }
}
