{
  "name": "@loopback/testlab",
  "version": "3.2.3",
  "description": "A collection of test utilities we use to write LoopBack tests.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-testlab*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-testlab*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@hapi/shot": "^5.0.1",
    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.1",
    "@types/shot": "^4.0.0",
    "@types/sinon": "^9.0.5",
    "@types/supertest": "^2.0.10",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "oas-validator": "^4.0.8",
    "should": "^13.2.3",
    "sinon": "^9.0.3",
    "supertest": "^4.0.2",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.1",
    "@loopback/eslint-config": "^9.0.1",
    "@types/node": "^10.17.28"
  },
  "files": [
    "README.md",
    "dist",
    "fixtures",
    "should-as-function.d.ts",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/testlab"
  }
}
