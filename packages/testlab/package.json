{
  "name": "@loopback/testlab",
  "version": "4.0.0-alpha.2",
  "description": "A collection of test utilities we use to write LoopBack tests.",
  "main": "index",
  "scripts": {
    "build": "npm run build:lib && npm run build:lib6",
    "build:lib": "tsc --target es2017 --outDir lib",
    "build:lib6": "tsc --target es2015 --outDir lib6",
    "clean": "rm *.tgz; rm -rf ./package",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha",
    "verify": "npm run clean && npm pack && tar xvzf *.tgz && tree package"
  },
  "author": "IBM",
  "license": "MIT",
  "devDependencies": {
    "mocha": "^3.3.0",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "@types/sinon": "^1.16.34",
    "@types/supertest": "^2.0.0",
    "should": "^11.2.1",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "lib",
    "lib6",
    "should-as-function.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
