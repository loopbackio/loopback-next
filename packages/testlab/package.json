{
  "name": "@loopback/testlab",
  "version": "4.0.0-alpha.12",
  "description": "A collection of test utilities we use to write LoopBack tests.",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "npm run build:dist && npm run build:dist6",
    "build:current": "lb-tsc",
    "build:dist": "lb-tsc es2017",
    "build:dist6": "lb-tsc es2015",
    "clean": "rm -rf loopback-testlab*.tgz dist* package",
    "prepare": "npm run build",
    "pretest": "npm run build:current",
    "test": "lb-dist mocha --recursive DIST/test",
    "verify": "npm pack && tar xf loopback-testlab*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "license": "MIT",
  "dependencies": {
    "@loopback/openapi-spec": "^4.0.0-alpha.13",
    "@types/shot": "^3.4.0",
    "@types/sinon": "^2.3.6",
    "@types/supertest": "^2.0.0",
    "@types/swagger-parser": "^4.0.1",
    "shot": "^4.0.2",
    "should": "^13.1.2",
    "sinon": "^4.0.1",
    "supertest": "^3.0.0",
    "swagger-parser": "^3.4.1"
  },
  "devDependencies": {
    "@loopback/build": "^4.0.0-alpha.3"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist6/src",
    "should-as-function.d.ts",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
