{
  "name": "@loopback/boot",
  "version": "1.4.0",
  "description": "A collection of Booters for LoopBack 4 Applications",
  "engines": {
    "node": ">=8.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "clean": "lb-clean loopback-boot*.tgz dist package api-docs",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-boot*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^1.19.0",
    "@loopback/core": "^1.8.1",
    "@loopback/repository": "^1.6.1",
    "@loopback/service-proxy": "^1.2.1",
    "@types/debug": "^4.1.4",
    "@types/glob": "^7.1.1",
    "debug": "^4.0.1",
    "glob": "^7.1.4"
  },
  "devDependencies": {
    "@loopback/build": "^1.7.1",
    "@loopback/eslint-config": "^1.1.1",
    "@loopback/openapi-v3": "^1.6.1",
    "@loopback/rest": "^1.15.0",
    "@loopback/testlab": "^1.5.0",
    "@types/node": "^10.11.2"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
