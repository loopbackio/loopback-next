{
  "name": "@loopback/boot",
  "description": "A collection of Booters for LoopBack 4 Applications",
  "version": "5.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/boot"
  },
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-boot*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-boot*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^4.0.0"
  },
  "dependencies": {
    "@loopback/model-api-builder": "^4.0.0",
    "@loopback/repository": "^5.0.0",
    "@loopback/service-proxy": "^5.0.0",
    "@types/debug": "^4.1.7",
    "@types/glob": "^7.2.0",
    "debug": "^4.3.4",
    "glob": "^8.0.3",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/eslint-config": "^13.0.0",
    "@loopback/rest": "^12.0.0",
    "@loopback/rest-crud": "^0.14.0",
    "@loopback/testlab": "^5.0.0",
    "@types/node": "^14.18.20"
  }
}
