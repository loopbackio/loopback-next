{
  "name": "@loopback/boot",
  "description": "A collection of Booters for LoopBack 4 Applications",
  "version": "3.4.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "The LoopBack Authors",
  "copyright.owner": "The LoopBack Authors",
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/boot"
  },
  "engines": {
    "node": "^10.16 || 12 || 14 || 16"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-boot*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-boot*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^2.16.1"
  },
  "dependencies": {
    "@loopback/model-api-builder": "^2.3.1",
    "@loopback/repository": "^3.7.0",
    "@loopback/service-proxy": "^3.2.1",
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.3",
    "debug": "^4.3.1",
    "glob": "^7.1.7",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.4.1",
    "@loopback/core": "^2.16.1",
    "@loopback/eslint-config": "^10.2.1",
    "@loopback/rest": "^9.3.1",
    "@loopback/rest-crud": "^0.10.1",
    "@loopback/testlab": "^3.4.1",
    "@types/node": "^10.17.60"
  }
}
