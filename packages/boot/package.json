{
  "name": "@loopback/boot",
  "version": "2.0.4",
  "description": "A collection of Booters for LoopBack 4 Applications",
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-boot*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-boot*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^3.4.0",
    "@loopback/core": "^2.3.0",
    "@loopback/model-api-builder": "^2.0.4",
    "@loopback/repository": "^2.1.1",
    "@loopback/service-proxy": "^2.0.4",
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.1",
    "debug": "^4.1.1",
    "glob": "^7.1.6",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@loopback/build": "^5.0.1",
    "@loopback/eslint-config": "^6.0.3",
    "@loopback/openapi-v3": "^3.1.3",
    "@loopback/rest": "^3.2.1",
    "@loopback/rest-crud": "^0.7.4",
    "@loopback/testlab": "^3.0.1",
    "@types/node": "^10.17.19"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/boot"
  }
}
