  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "stoppable": "^1.1.0",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.0",
    "@loopback/core": "^2.9.3",
    "@loopback/eslint-config": "^9.0.0",
    "@loopback/testlab": "^3.2.2",
    "@types/node": "^10.17.28",
    "@types/stoppable": "^1.1.0"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
