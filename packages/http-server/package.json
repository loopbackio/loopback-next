{
  "name": "@loopback/http-server",
  "description": "A wrapper for creating HTTP/HTTPS servers",
  "version": "3.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/http-server"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-http-server*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-http-server*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "debug": "^4.3.3",
    "stoppable": "^1.1.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.0",
    "@loopback/core": "^3.0.0",
    "@loopback/eslint-config": "^12.0.0",
    "@loopback/testlab": "^4.0.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^10.17.60",
    "@types/stoppable": "^1.1.1"
  }
}
