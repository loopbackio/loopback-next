  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^0.2.1",
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^0.2.0",
    "@loopback/testlab": "^0.3.0",
    "@types/debug": "^0.0.30"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "api-docs",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
