{
  "name": "@loopback/http-server",
  "version": "2.1.11",
  "description": "A wrapper for creating HTTP/HTTPS servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
