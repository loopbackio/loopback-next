{
  "name": "@loopback/http-server",
  "version": "1.1.3",
  "description": "A wrapper for creating HTTP/HTTPS servers",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "clean": "lb-clean loopback-http-server*.tgz dist package api-docs",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/test/**/*.js\"",
    "verify": "npm pack && tar xf loopback-http-server*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "p-event": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.2.0",
    "@loopback/core": "^1.1.4",
    "@loopback/testlab": "^1.0.4",
    "@loopback/tslint-config": "^2.0.0",
    "@types/node": "^10.11.2",
    "@types/p-event": "^1.3.0",
    "@types/request-promise-native": "^1.0.15",
    "request-promise-native": "^1.0.5"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
