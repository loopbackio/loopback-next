{
  "name": "@loopback/http-server",
  "description": "A wrapper for creating HTTP/HTTPS servers",
  "version": "6.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/http-server"
  },
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-http-server*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-http-server*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "debug": "^4.3.5",
    "stoppable": "^1.1.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.2",
    "@loopback/core": "^6.0.2",
    "@loopback/eslint-config": "^15.0.2",
    "@loopback/testlab": "^7.0.2",
    "@types/debug": "^4.1.12",
    "@types/node": "^16.18.97",
    "@types/stoppable": "^1.1.3"
  }
}
