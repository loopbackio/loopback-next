{
  "name": "@loopback/http-caching-proxy",
  "version": "2.2.1",
  "description": "A caching HTTP proxy for integration tests. NOT SUITABLE FOR PRODUCTION USE!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^10.16 || 12 || 14 || 16"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-caching-proxy*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-caching-proxy*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "cacache": "^15.0.6",
    "debug": "^4.3.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.1",
    "@loopback/eslint-config": "^10.1.1",
    "@loopback/testlab": "^3.3.1",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.59",
    "@types/rimraf": "^3.0.0",
    "@types/tunnel": "0.0.2",
    "delay": "^5.0.0",
    "tunnel": "0.0.6"
  },
  "keywords": [
    "LoopBack",
    "HTTP",
    "Proxy",
    "Cache",
    "Test"
  ],
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/http-caching-proxy"
  }
}
