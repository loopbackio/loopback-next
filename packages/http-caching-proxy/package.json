{
  "name": "@loopback/http-caching-proxy",
  "description": "A caching HTTP proxy for integration tests. NOT SUITABLE FOR PRODUCTION USE!",
  "version": "3.1.0",
  "keywords": [
    "LoopBack",
    "HTTP",
    "Proxy",
    "Cache",
    "Test"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/http-caching-proxy"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-caching-proxy*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-caching-proxy*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "axios": "^0.25.0",
    "cacache": "^15.3.0",
    "debug": "^4.3.3",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.0",
    "@loopback/eslint-config": "^12.0.2",
    "@loopback/testlab": "^4.1.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^12.20.46",
    "@types/rimraf": "^3.0.2",
    "@types/tunnel": "0.0.3",
    "delay": "^5.0.0",
    "tunnel": "0.0.6"
  }
}
