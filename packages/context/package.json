{
  "name": "@loopback/context",
  "version": "4.0.0-alpha.15",
  "description": "LoopBack's container for Inversion of Control",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "acceptance": "lb-dist mocha --opts ../../test/mocha.opts 'DIST/test/acceptance/**/*.js'",
    "build": "npm run build:dist && npm run build:dist6",
    "build:current": "lb-tsc",
    "build:dist": "lb-tsc es2017",
    "build:dist6": "lb-tsc es2015",
    "build:apidocs": "lb-apidocs",
    "clean": "rm -rf loopback-context*.tgz dist* package",
    "prepublish": "npm run build && npm run build:apidocs",
    "pretest": "npm run build:current",
    "test": "lb-dist mocha --opts ../../test/mocha.opts 'DIST/test/unit/**/*.js' 'DIST/test/acceptance/**/*.js'",
    "unit": "lb-dist mocha --opts ../../test/mocha.opts 'DIST/test/unit/**/*.js'",
    "verify": "npm pack && tar xf loopback-context*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@loopback/build": "^4.0.0-alpha.1",
    "@loopback/testlab": "^4.0.0-alpha.10",
    "@types/bluebird": "^3.5.8",
    "bluebird": "^3.5.0"
  },
  "keywords": [
    "LoopBack",
    "IoC",
    "Inversion",
    "Control",
    "Container",
    "Decorators",
    "Context"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist6/src",
    "api-docs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
