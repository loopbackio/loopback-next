{
  "name": "@loopback/context",
  "version": "4.0.0-alpha.5",
  "description": "LoopBack's container for Inversion of Control",
  "main": "index",
  "scripts": {
    "acceptance": "mocha --opts ../../test/mocha.opts 'test/acceptance/**/*.ts'",
    "build": "npm run build:lib && npm run build:lib6",
    "build:lib": "tsc --target es2017 --outDir lib",
    "build:lib6": "tsc --target es2015 --outDir lib6",
    "clean": "rm -f loopback-context*.tgz && rm -rf lib*",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha --opts ../../test/mocha.opts 'test/unit/**/*.ts' 'test/acceptance/**/*.ts'",
    "unit": "mocha --opts ../../test/mocha.opts 'test/unit/**/*.ts'",
    "verify": "npm run clean && npm pack && tar tf loopback-context*.tgz | sort"
  },
  "author": "IBM",
  "license": "MIT",
  "dependencies": {
    "is-promise": "^2.1.0",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@loopback/testlab": "^4.0.0-alpha.2",
    "@types/bluebird": "^3.5.2",
    "bluebird": "^3.5.0",
    "mocha": "^3.2.0",
    "typescript": "^2.3.2"
  },
  "keywords": [
    "LoopBack",
    "IoC",
    "Inversion",
    "Control",
    "Container",
    "Decorators",
    "Context"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "lib",
    "lib6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
