{
  "name": "@loopback/context",
  "version": "1.24.0",
  "description": "LoopBack's container for Inversion of Control",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-context*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-context*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/metadata": "^1.3.8",
    "debug": "^4.1.1",
    "p-event": "^4.1.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@loopback/build": "^2.1.0",
    "@loopback/eslint-config": "^4.1.5",
    "@loopback/testlab": "^1.9.5",
    "@types/bluebird": "^3.5.29",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.5",
    "@types/uuid": "^3.4.6",
    "bluebird": "^3.7.1"
  },
  "keywords": [
    "LoopBack",
    "IoC",
    "Inversion",
    "Control",
    "Container",
    "Decorators",
    "Context"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/context"
  }
}
