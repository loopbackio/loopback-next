  "devDependencies": {
    "@loopback/build": "^1.7.1",
    "@loopback/core": "^2.2.0",
    "@loopback/repository": "^2.0.2",
    "@loopback/rest": "^3.1.0",
    "@loopback/testlab": "^2.0.2",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.19"
  },
  "peerDependencies": {
    "@loopback/core": "^1.12.4",
    "@loopback/repository": "^1.19.1",
    "@loopback/rest": "^2.0.0"
