{
  "name": "@loopback/rest",
  "version": "4.0.0-alpha.4",
  "description": "",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "acceptance": "lb-dist mocha --opts ../../test/mocha.opts 'DIST/test/acceptance/**/*.js'",
    "build": "npm run build:dist && npm run build:dist6",
    "build:current": "lb-tsc",
    "build:dist": "lb-tsc es2017",
    "build:dist6": "lb-tsc es2015",
    "build:apidocs": "lb-apidocs",
    "clean": "rm -rf loopback-rest*.tgz dist* package",
    "prepublish": "npm run build && npm run build:apidocs",
    "pretest": "npm run build:current",
    "integration": "lb-dist mocha --opts ../../test/mocha.opts 'DIST/test/integration/**/*.js'",
    "test": "lb-dist mocha --opts ../../test/mocha.opts 'DIST/test/unit/**/*.js' 'DIST/test/integration/**/*.js' 'DIST/test/acceptance/**/*.js'",
    "unit": "lb-dist mocha --opts ../../test/mocha.opts 'DIST/test/unit/**/*.js'",
    "verify": "npm pack && tar xf loopback-rest*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^4.0.0-alpha.15",
    "@loopback/core": "^4.0.0-alpha.17",
    "@loopback/openapi-spec": "^4.0.0-alpha.11",
    "@types/http-errors": "^1.5.34",
    "@types/js-yaml": "^3.9.1",
    "body": "^5.1.0",
    "debug": "^3.1.0",
    "http-errors": "^1.6.1",
    "js-yaml": "^3.9.1",
    "lodash": "^4.17.4",
    "path-to-regexp": "^2.0.0",
    "swagger2openapi": "^2.9.2"
  },
  "devDependencies": {
    "@loopback/build": "^4.0.0-alpha.1",
    "@loopback/openapi-spec-builder": "^4.0.0-alpha.8",
    "@loopback/testlab": "^4.0.0-alpha.10"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist6/src",
    "api-docs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
