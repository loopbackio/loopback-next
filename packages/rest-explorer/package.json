{
  "name": "@loopback/rest-explorer",
  "description": "LoopBack's API Explorer",
  "version": "8.0.6",
  "keywords": [
    "LoopBack",
    "Explorer",
    "Swagger"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/rest-explorer"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-explorer*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-explorer*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__",
    "templates"
  ],
  "peerDependencies": {
    "@loopback/core": "^7.0.0",
    "@loopback/rest": "^15.0.1"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "swagger-ui-dist": "5.30.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.5",
    "@loopback/core": "^7.0.5",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/rest": "^15.0.6",
    "@loopback/testlab": "^8.0.5",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.25",
    "@types/node": "^16.18.126",
    "express": "^4.21.2"
  }
}
