{
  "name": "@loopback/rest-explorer",
  "description": "LoopBack's API Explorer",
  "version": "4.1.0",
  "keywords": [
    "LoopBack",
    "Explorer",
    "Swagger"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/rest-explorer"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-explorer*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-explorer*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__",
    "templates"
  ],
  "peerDependencies": {
    "@loopback/core": "^3.1.0",
    "@loopback/rest": "^11.1.0"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "swagger-ui-dist": "4.5.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.0",
    "@loopback/core": "^3.1.0",
    "@loopback/eslint-config": "^12.0.2",
    "@loopback/rest": "^11.1.0",
    "@loopback/testlab": "^4.1.0",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/node": "^12.20.46",
    "express": "^4.17.2"
  }
}
