{
  "name": "@loopback/metadata",
  "version": "1.4.1",
  "description": "LoopBack's metadata utilities for reflection and decoration",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-metadata*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-metadata*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.11.0"
  },
  "devDependencies": {
    "@loopback/build": "^3.1.1",
    "@loopback/eslint-config": "^5.0.3",
    "@loopback/testlab": "^1.10.3",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.17.16"
  },
  "keywords": [
    "LoopBack",
    "Decorators",
    "Reflect"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/metadata"
  }
}
