    "@types/http-errors": "^1.8.0",
    "body-parser": "^1.19.0",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "on-finished": "^2.3.0",
    "toposort": "^2.0.2",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.4.0",
