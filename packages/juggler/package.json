{
  "name": "@loopback/juggler",
  "version": "4.0.0-alpha.1",
  "description": "Juggler for LoopBack.next",
  "main": "index",
  "scripts": {
    "acceptance": "mocha --opts ../../test/mocha.opts 'test/acceptance/**/*.ts'",
    "build": "npm run build:lib && npm run build:lib6",
    "build:lib": "tsc --target es2017 --outDir lib",
    "build:lib6": "tsc --target es2015 --outDir lib6",
    "clean": "rm -f loopback-juggler*.tgz && rm -rf lib*",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "mocha --opts ../../test/mocha.opts 'test/unit/**/*.ts' 'test/acceptance/**/*.ts'",
    "unit": "mocha --opts ../../test/mocha.opts 'test/unit/**/*.ts'",
    "verify": "npm run clean && npm pack && tar tf loopback-juggler*.tgz | sort"
  },
  "author": "IBM",
  "license": "MIT",
  "devDependencies": {
    "@loopback/testlab": "^4.0.0-alpha.2",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "@loopback/context": "^4.0.0-alpha.6",
    "loopback-datasource-juggler": "^3.6.1"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "lib",
    "lib6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
