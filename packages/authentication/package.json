{
  "name": "@loopback/authentication",
  "version": "4.2.6",
  "description": "A LoopBack component for authentication support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-authentication*.tgz dist *.tsbuildinfo package",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-authentication*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/core": "^2.7.1",
    "@loopback/metadata": "^2.1.6",
    "@loopback/openapi-v3": "^3.4.2",
    "@loopback/rest": "^5.1.0",
    "@loopback/security": "^0.2.11",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.154",
    "lodash": "^4.17.15",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.4.2",
    "@loopback/eslint-config": "^8.0.0",
    "@loopback/openapi-spec-builder": "^2.1.6",
    "@loopback/testlab": "^3.1.6",
    "@types/node": "^10.17.24",
    "jsonwebtoken": "^8.5.1"
  },
  "keywords": [
    "LoopBack",
    "Authentication"
  ],
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/authentication"
  }
}
