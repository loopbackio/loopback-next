{
  "name": "@loopback/authentication",
  "version": "3.2.4",
  "description": "A LoopBack component for authentication support.",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-authentication*.tgz dist tsconfig.build.tsbuildinfo package",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-authentication*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^1.25.0",
    "@loopback/core": "^1.12.0",
    "@loopback/metadata": "^1.3.9",
    "@loopback/openapi-v3": "^1.10.3",
    "@loopback/rest": "^1.25.0",
    "@loopback/security": "^0.1.9",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@loopback/build": "^3.0.0",
    "@loopback/eslint-config": "^5.0.0",
    "@loopback/openapi-spec-builder": "^1.2.20",
    "@loopback/testlab": "^1.10.0",
    "@types/node": "^10.17.13",
    "jsonwebtoken": "^8.5.1"
  },
  "keywords": [
    "LoopBack",
    "Authentication"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/authentication"
  }
}
