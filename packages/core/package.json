{
  "name": "@loopback/core",
  "version": "2.4.2",
  "description": "LoopBack 4 core",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-core*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-core*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^3.6.0",
    "debug": "^4.1.1",
    "p-event": "^4.1.0",
    "tslib": "^1.11.2"
  },
  "devDependencies": {
    "@loopback/build": "^5.3.0",
    "@loopback/eslint-config": "^6.0.5",
    "@loopback/testlab": "^3.1.2",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.21"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/core"
  }
}
