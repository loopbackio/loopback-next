  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^1.19.0",
    "debug": "^4.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.7.1",
    "@loopback/eslint-config": "^1.1.1",
    "@loopback/testlab": "^1.5.0",
    "@types/debug": "^4.1.4",
    "@types/node": "^10.11.2"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
