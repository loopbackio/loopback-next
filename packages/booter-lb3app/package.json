{
  "name": "@loopback/booter-lb3app",
  "version": "1.3.2",
  "description": "Boot a LoopBack 3 application in a LoopBack 4 project",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-booter-lb3app*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-booter-lb3app*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.1",
    "debug": "^4.1.1",
    "loopback": "^3.26.0",
    "loopback-swagger": "^5.7.3",
    "p-event": "^4.1.0",
    "swagger2openapi": "^5.3.1"
  },
  "peerDependencies": {
    "@loopback/boot": "^1.2.2",
    "@loopback/core": "^1.5.0",
    "@loopback/rest": "^1.10.3"
  },
  "devDependencies": {
    "@loopback/boot": "^1.5.8",
    "@loopback/build": "^2.0.13",
    "@loopback/core": "^1.10.4",
    "@loopback/eslint-config": "^4.1.1",
    "@loopback/repository": "^1.15.1",
    "@loopback/rest": "^1.20.1",
    "@loopback/testlab": "^1.9.1",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.141",
    "@types/node": "^10.14.19",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^3.21.1",
    "loopback-boot": "^3.3.1"
  },
  "keywords": [
    "LoopBack",
    "LB3",
    "Boot",
    "Compatibility"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/booter-lb3app"
  }
}
