  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "debug": "^4.1.1",
    "loopback": "^3.27.0",
    "loopback-swagger": "^5.9.0",
    "swagger2openapi": "^6.0.3",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@loopback/boot": "^2.3.2",
    "@loopback/core": "^2.7.1",
    "@loopback/rest": "^5.1.0"
  },
  "devDependencies": {
    "@loopback/boot": "^2.3.2",
    "@loopback/build": "^5.4.2",
    "@loopback/core": "^2.7.1",
    "@loopback/eslint-config": "^8.0.0",
    "@loopback/repository": "^2.6.0",
    "@loopback/rest": "^5.1.0",
    "@loopback/testlab": "^3.1.6",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.155",
    "@types/node": "^10.17.25",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^3.22.0",
    "loopback-boot": "^3.3.1"
  },
  "keywords": [
    "LoopBack",
    "LB3",
    "Boot",
    "Compatibility"
