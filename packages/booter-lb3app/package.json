{
  "name": "@loopback/booter-lb3app",
  "version": "1.0.0-1",
  "description": "Boot a LoopBack 3 application in a LoopBack 4 project",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "clean": "lb-clean loopback-booter-lb3app*.tgz dist package api-docs",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-booter-lb3app*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.16.1",
    "debug": "^4.1.1",
    "loopback": "^3.25.1",
    "loopback-swagger": "^5.7.3",
    "p-event": "^4.1.0",
    "swagger2openapi": "^5.2.5"
  },
  "peerDependencies": {
    "@loopback/boot": "^1.2.2",
    "@loopback/core": "^1.5.0",
    "@loopback/rest": "^1.10.3"
  },
  "devDependencies": {
    "@loopback/boot": "^1.2.2",
    "@loopback/build": "^1.5.0",
    "@loopback/core": "^1.5.0",
    "@loopback/repository": "^1.5.0",
    "@loopback/rest": "^1.10.3",
    "@loopback/testlab": "^1.2.5",
    "@loopback/tslint-config": "^2.0.4",
    "@types/debug": "^4.1.4",
    "@types/node": "^10.11.2",
    "@types/lodash": "^4.14.123",
    "loopback-boot": "^3.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^3.16.0"
  },
  "keywords": [
    "LoopBack",
    "LB3",
    "Boot",
    "Compatibility"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
