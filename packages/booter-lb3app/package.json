{
  "name": "@loopback/booter-lb3app",
  "description": "A booter component for LoopBack 3 applications to expose their REST API via LoopBack 4",
  "version": "7.0.9",
  "keywords": [
    "LoopBack",
    "LB3",
    "Boot",
    "Compatibility"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "packages/booter-lb3app"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-booter-lb3app*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-booter-lb3app*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/boot": "^8.0.1",
    "@loopback/core": "^7.0.0",
    "@loopback/rest": "^15.0.1"
  },
  "dependencies": {
    "@types/express": "^4.17.25",
    "debug": "^4.4.3",
    "loopback": "^3.28.0",
    "loopback-swagger": "^5.9.0",
    "swagger2openapi": "^7.0.8",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/boot": "^8.0.9",
    "@loopback/build": "^12.0.8",
    "@loopback/core": "^7.0.8",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/repository": "^8.0.8",
    "@loopback/rest": "^15.0.9",
    "@loopback/testlab": "^8.0.8",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.23",
    "@types/node": "^16.18.126",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "helmet": "^4.6.0",
    "loopback-boot": "^3.3.1"
  }
}
