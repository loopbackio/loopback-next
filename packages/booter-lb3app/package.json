{
  "name": "@loopback/booter-lb3app",
  "description": "A booter component for LoopBack 3 applications to expose their REST API via LoopBack 4",
  "version": "2.4.0",
  "keywords": [
    "LoopBack",
    "LB3",
    "Boot",
    "Compatibility"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/booter-lb3app"
  },
  "engines": {
    "node": "^10.16 || 12 || 14 || 16"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean loopback-booter-lb3app*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-booter-lb3app*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/boot": "^3.4.0",
    "@loopback/core": "^2.16.0",
    "@loopback/rest": "^9.3.0"
  },
  "dependencies": {
    "@types/express": "^4.17.12",
    "debug": "^4.3.1",
    "loopback": "^3.28.0",
    "loopback-swagger": "^5.9.0",
    "swagger2openapi": "^7.0.5",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@loopback/boot": "^3.4.0",
    "@loopback/build": "^6.4.0",
    "@loopback/core": "^2.16.0",
    "@loopback/eslint-config": "^10.2.0",
    "@loopback/repository": "^3.6.0",
    "@loopback/rest": "^9.3.0",
    "@loopback/testlab": "^3.4.0",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.170",
    "@types/node": "^10.17.60",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^4.6.0",
    "loopback-boot": "^3.3.1"
  }
}
