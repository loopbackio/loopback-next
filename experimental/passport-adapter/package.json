{
  "name": "@loopback/passport-adapter",
  "version": "0.1.0",
  "description": "A package creating adapters between the passport module and @loopback/authentication",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "clean": "lb-clean loopback-passport-adapter*.tgz dist package api-docs",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-passport-adapter*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "keywords": [
    "Passport",
    "Authentication",
    "TypeScript"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-labs.git"
  },
  "dependencies": {
    "@loopback/authentication": "^2.0.0",
    "@loopback/rest": "^1.11.2",
    "passport": "^0.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.5.5",
    "@loopback/openapi-spec-builder": "^1.1.11",
    "@loopback/testlab": "^1.2.10",
    "passport-http": "^0.3.0"
  }
}
