{
  "name": "@loopback/benchmark",
  "description": "Benchmarks measuring performance of our framework.",
  "version": "7.0.1",
  "keywords": [
    "loopback",
    "performance",
    "benchmark"
  ],
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "benchmark"
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "clean": "lb-clean dist *.tsbuildinfo",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "prestart": "npm run build",
    "benchmark:routing": "node ./dist/rest-routing/routing-table",
    "benchmark:context": "node ./dist/context-binding/context-binding",
    "start": "node ."
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/core": "^7.0.1",
    "@loopback/example-todo": "^9.0.1",
    "@loopback/openapi-spec-builder": "^8.0.1",
    "@loopback/rest": "^15.0.1",
    "@types/byline": "^4.2.36",
    "@types/debug": "^4.1.12",
    "@types/request-promise-native": "^1.0.21",
    "autocannon": "^8.0.0",
    "axios": "^1.9.0",
    "benchmark": "^2.1.4",
    "byline": "^5.0.0",
    "debug": "^4.4.1",
    "path-to-regexp": "^6.3.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.1",
    "@loopback/testlab": "^8.0.1",
    "@types/autocannon": "^7.12.7",
    "@types/benchmark": "^2.1.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^16.18.126",
    "mocha": "^11.6.0",
    "source-map-support": "^0.5.21"
  }
}
