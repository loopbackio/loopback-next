{
  "name": "@loopback/benchmark",
  "version": "1.1.13",
  "private": true,
  "description": "Benchmarks measuring performance of our framework.",
  "keywords": [
    "loopback",
    "performance",
    "benchmark"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc es2017 --outDir dist",
    "clean": "lb-clean dist",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "prestart": "npm run build",
    "benchmark:routing": "node ./dist/rest-routing/routing-table",
    "start": "node ."
  },
  "repository": {
    "type": "git"
  },
  "author": "IBM Corp.",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/example-todo": "^1.5.1",
    "@loopback/openapi-spec-builder": "^1.1.0",
    "@loopback/rest": "^1.8.0",
    "@types/byline": "^4.2.31",
    "@types/debug": "^4.1.0",
    "@types/request-promise-native": "^1.0.15",
    "autocannon": "^3.0.0",
    "byline": "^5.0.0",
    "debug": "^4.0.1",
    "path-to-regexp": "^3.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.2",
    "@loopback/testlab": "^1.1.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.11.2",
    "mocha": "^6.0.0",
    "p-event": "^4.0.0",
    "source-map-support": "^0.5.5"
  }
}
